{
  "name": "mocha-spec-json-output-reporter",
  "description": "mocha spec + json output file reporter",
  "scripts": {
    "sample-test": "mocha test/sample-test.js -R mocha-spec-json-output-reporter --reporter-options fileName=mocha-output.json",
    "sample-test-hierarchy": "mocha test/sample-test.js -R mocha-spec-json-output-reporter --reporter-options fileName=mocha-output-hierarchy.json,hierarchy=true",
    "sample-test-all": "mocha test/sample-test*.js -R mocha-spec-json-output-reporter --reporter-options fileName=mocha-output-hierarchy.json,hierarchy=true",
    "test": "mocha test/validate-output*.js",
    "publish": "npm publish lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vchin/mocha-spec-json-output-reporter.git"
  },
  "author": "vchin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vchin/mocha-spec-json-output-reporter/issues"
  },
  "homepage": "https://github.com/vchin/mocha-spec-json-output-reporter#readme",
  "devDependencies": {
    "chai": "^4.1.2",
    "mocha": "^5.0.4",
    "mocha-spec-json-output-reporter": "file:./lib"
  }
}
